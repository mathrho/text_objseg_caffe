layer {
  name: "lstm_feat"
  type: "Reshape"
  bottom: "lstm_out"
  top: "lstm_feat"
  reshape_param {
    shape {
      dim: -1
      dim: 1000
    }
  }
}
layer {
  name: "dyn_l"
  type: "InnerProduct"
  bottom: "lstm_feat"
  top: "dyn_l"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1008
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "dyn_sig"
  type: "Sigmoid"
  bottom: "dyn_l"
  top: "dyn_l"
}
layer {
  name: "dyn_sig_l2norm"
  type: "L2Normalize"
  bottom: "dyn_l"
  top: "dyn_sig_l2norm"
}
layer {
  name: "dyn_sig_l2norm_resh"
  type: "Reshape"
  bottom: "dyn_sig_l2norm"
  top: "dyn_sig_l2norm_resh"
  reshape_param {
    shape {
      dim: -1
      dim: 1008
    }
  }
}
layer {
  name: "feat_all"
  type: "Concat"
  bottom: "img_feature"
  bottom: "spatial"
  top: "feat_all"
  concat_param {
    axis: 1
  }
}
layer {
  name: "feat_all_l2norm"
  type: "L2Normalize"
  bottom: "feat_all"
  top: "feat_all_l2norm"
}
layer {
  name: "feat_all_l2norm_resh"
  type: "Reshape"
  bottom: "feat_all_l2norm"
  top: "feat_all_l2norm_resh"
  reshape_param {
    shape {
      dim: -1
      dim: 1008
    }
  }
}
layer {
  name: "scores"
  type: "DotProductSimilarity"
  bottom: "dyn_sig_l2norm_resh"
  bottom: "feat_all_l2norm_resh"
  top: "scores"
}
layer {
  name: "loss"
  type: "SigmoidCrossEntropyLoss"
  bottom: "scores"
  bottom: "label"
  top: "loss"
}
