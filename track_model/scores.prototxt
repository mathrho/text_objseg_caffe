layer {
  name: "language"
  type: "Python"
  top: "language"
  top: "cont"
  top: "img_feature"
  top: "spatial"
  top: "label"
  python_param {
    module: "referit_data_provider"
    layer: "TossLayer"
    param_str: "{\'split\': \'val\', \'batch_size\': 100}"
  }
}
layer {
  name: "embed"
  type: "Embed"
  bottom: "language"
  top: "embed"
  embed_param {
    num_output: 1000
    input_dim: 8803
    weight_filler {
      type: "uniform"
      min: -0.08
      max: 0.08
    }
  }
}
layer {
  name: "lstm"
  type: "LSTM"
  bottom: "embed"
  bottom: "cont"
  top: "lstm"
  recurrent_param {
    num_output: 1000
    weight_filler {
      type: "uniform"
      min: -0.08
      max: 0.08
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "slice0"
  type: "Slice"
  bottom: "lstm"
  top: "slice0"
  top: "slice1"
  top: "slice2"
  top: "slice3"
  top: "slice4"
  top: "slice5"
  top: "slice6"
  top: "slice7"
  top: "slice8"
  top: "slice9"
  top: "slice10"
  top: "slice11"
  top: "slice12"
  top: "slice13"
  top: "slice14"
  top: "slice15"
  top: "slice16"
  top: "slice17"
  top: "slice18"
  top: "lstm_out"
  slice_param {
    axis: 0
  }
}
layer {
  name: "silence0"
  type: "Silence"
  bottom: "slice0"
}
layer {
  name: "silence1"
  type: "Silence"
  bottom: "slice1"
}
layer {
  name: "silence2"
